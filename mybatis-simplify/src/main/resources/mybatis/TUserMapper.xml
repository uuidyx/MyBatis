<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="ren.oliver.mybatis.simplify.mapper.TUserMapper">

    <select id="selectByPrimaryKey" resultType="ren.oliver.mybatis.simplify.entity.TUser">
		select
		id, userName, realName, sex, mobile, email, note
		from t_user
		where id =?
	</select>

    <select id="selectAll" resultType="ren.oliver.mybatis.simplify.entity.TUser">
		select
		id, userName, realName, sex, mobile, email, note
		from t_user
	</select>

</mapper>